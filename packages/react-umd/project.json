{
  "name": "react-umd",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/react-umd/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/react-umd/jest.config.ts"
      }
    },
    "build-umd": {
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/dist/packages/react-umd"],
      "options": {
        "command": "vite build --config vite.config.umd.ts",
        "cwd": "packages/react-umd"
      }
    },
    "vite-typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --noEmit -p packages/react-umd/tsconfig.app.json"
      }
    }
  }
}
